    "keywords": [
        "firebase",
        "eloquent",
        "laravel"
    ],
    "homepage": "http://github.com/castro732/laravel-firebase-synchronization",
    "authors": [
        {
            "name": "Jose M",
            "email": "castro732@gmail.com"
        }
    ],
    "support": {
        "issues": "https://github.com/castro732/laravel-firebase-synchronization/issues",
        "source": "https://github.com/castro732/laravel-firebase-synchronization"
    },
    "require": {
        "php": ">=7.0.0",
        "illuminate/support": "~5.0",
        "kreait/firebase-php": "^3.4.2"
    },
    "require-dev": {
        "mockery/mockery": "~0.9",
        "orchestra/testbench": "~3.0",
        "phpunit/phpunit": "~4.7 || ~5.0"
